  shortjournal = {Eur Child Adolesc Psychiatry},
  urldate = {2016-08-17},
  date = {2011-05-22},
  pages = {361-371},
  author = {Wijsbroek, Saskia A. M. and Hale, William W. and Raaijmakers, Quinten A. W. and Meeus, Wim H. J.},
