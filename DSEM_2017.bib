@article{deutschPaternalParticipationChild2001,
  langid = {english},
  title = {Paternal {{Participation}} in {{Child Care}} and {{Its Effects}} on {{Children}}'s {{Self}}-{{Esteem}} and {{Attitudes Toward Gendered Roles}}},
  volume = {22},
  issn = {0192-513X},
  url = {https://doi.org/10.1177/019251301022008003},
  doi = {10.1177/019251301022008003},
  abstract = {Interviews with forty 10- and 11-year-old children (24 boys and 16 girls) investigated the effects of parents' division of child care responsibilities on children's self-esteem, their relationships with parents, and their gendered ideas and preferences. Children whose fathers participated relatively more in the emotional side of parenting (e.g., comforting) showed greater preferences for “feminine” activities and had higher self-esteem than children whose fathers were less involved. Children whose fathers performed a higher proportion of the “work” of parenting (e.g., transporting, planning activities, and arranging child care) endorsed a more gender-free model of family life. The absolute amount of time fathers spent with children had no independent significant effects. Egalitarian parenting clearly benefits children when fathers share “maternal” tasks, but even when fathers do not fully participate in those “maternal” aspects of parenting, dividing the time 50-50 may benefit mothers without hurting children.},
  number = {8},
  journaltitle = {Journal of Family Issues},
  shortjournal = {Journal of Family Issues},
  date = {2001-11-01},
