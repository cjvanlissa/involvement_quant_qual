@article{pomerantzRoleMaternalControl1998,
  eprinttype = {jstor},
  eprint = {1132178},
  title = {The {{Role}} of {{Maternal Control}} in the {{Development}} of {{Sex Differences}} in {{Child Self}}-{{Evaluative Factors}}},
  volume = {69},
  issn = {0009-3920},
  doi = {10.2307/1132178},
  abstract = {The major goal of the present research was to examine the role of parental control in the development of sex differences in 2 pivotal self-evaluative factors in children: taking responsibility for failure and possessing strong standards. Parents were expected to use control with and without autonomy granting in specific domains differentially with girls and boys on a daily basis, and this was expected to foster sex differences in children's self-evaluative factors. Ninety-one mothers of elementary school children completed a daily checklist for 10 to 21 days. The checklist assessed mothers' everyday use of control and autonomy granting in 5 specific domains (helping, monitoring, decision making, praising, and disciplining). Children completed self-report measures assessing their tendency to take responsibility for failure and the strength of their standards. As anticipated, mothers were more likely to employ control without autonomy granting with girls than with boys, but were more likely to employ control with autonomy granting with boys than with girls. Significantly, this pattern of gender socialization partially mediated the tendency of girls to take greater responsibility for failure than boys. Although there were no sex differences in the strength of children's standards, the pattern of gender socialization was associated with the strength of children's standards as well.},
  number = {2},
  journaltitle = {Child Development},
  shortjournal = {Child Development},
  date = {1998},
  pages = {458-478},
  author = {Pomerantz, Eva M. and Ruble, Diane N.},
  file = {C\:\\Users\\lissa102\\Zotero\\storage\\TMU9MPB9\\Pomerantz_Ruble_1998_The Role of Maternal Control in the Development of Sex Differences in Child.pdf}
}

